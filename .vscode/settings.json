{ // editor
"editor.wordwrap": "on",
"editor.fontSize": "18",
"editor.lineHeight": "30",
"editor.tabSize": "2",
"editor.backetPairColorization.enable": true,
"editor.guides.backetPairs": true,
"editor.minimap.enable": false,
"editor.formatOneSave": true,
"editor.formatOnePaste": true,






